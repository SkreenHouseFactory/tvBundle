<!doctype html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="fr"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="fr"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="fr"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="fr"> <!--<![endif]-->
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

	<title>{% block title endblock %} | {{ 'service.name'|trans }}</title>
	<meta name="description" content="{% block description %}{% endblock %}">
	<meta name="author" content="SkreenHouseFactory">
	<meta name="language" content="fr-FR" />
	<meta name="robots" content="noindex,nofollow">
	{% block meta %}{% endblock %}

  {% stylesheets
      '@SkreenHouseFactoryV3Bundle/Resources/public/initializr/less/modal.less'
      '@SkreenHouseFactoryV3Bundle/Resources/public/initializr/less/slider.less'
      '@SkreenHouseFactoryV3Bundle/Resources/public/initializr/less/bootstrap/bootstrap.less'
      '@SkreenHouseFactoryV3Bundle/Resources/public/initializr/less/bootstrap/responsive.less'
        filter='less,?yui_css'
        output='css/style-core.css'
        %}
      <link href="{{ asset_url }}" rel="stylesheet" media="screen" />
  {% endstylesheets %}
  {% stylesheets
      '@SkreenHouseFactoryTvBundle/Resources/public/less/tv.less'
        filter='less,?yui_css'
        output='css/tv.css'
        %}
      <link href="{{ asset_url }}" rel="stylesheet" media="screen" />
  {% endstylesheets %}
  {% if device['head'] is defined %}
    {% include device['head'] %}
  {% endif %}
</head>
<body>
<!--[if lt IE 7]><p class=chromeframe>Votre navigateur est <em>ancien!</em> <a href="http://browsehappy.com/">Mettez-le Ã  jour</a> ou <a href="http://www.google.com/chromeframe/?redirect=true">installez Google Chrome Frame</a> pour profiter utiliser mySkreen dans les meilleurs conditions.</p><![endif]-->

  {% include 'SkreenHouseFactoryV3Bundle:Main:_modal.html.twig' %}
  {% include 'SkreenHouseFactoryTvBundle:Main:_topbar.html.twig' %}

  <div id="sliders" class="template tv-container">
    <div class="container"></div>
  </div>
  {#<div id="redirect" class="template"></div>#}
  {% include 'SkreenHouseFactoryTvBundle:Main:_couchmode.html.twig' %}

  {% block content %}
  {% endblock %}

{% javascripts
    '@SkreenHouseFactoryV3Bundle/Resources/public/initializr/js/libs/modernizr-2.5.3-respond-1.1.0.min.js'
    '@SkreenHouseFactoryV3Bundle/Resources/public/initializr/js/libs/jquery-1.7.2.min.js'
    '@SkreenHouseFactoryV3Bundle/Resources/public/initializr/js/libs/bootstrap/first/*'
    '@SkreenHouseFactoryV3Bundle/Resources/public/initializr/js/libs/bootstrap/last/*'
    '@SkreenHouseFactoryTvBundle/Resources/public/js/vendors/jquery.keynav.js'
    '@SkreenHouseFactoryV3Bundle/Resources/public/js/vendors/*'
    '@SkreenHouseFactoryV3Bundle/Resources/public/js/core/*'
    '@SkreenHouseFactoryTvBundle/Resources/public/js/*' 
    filter='?yui_js' 
    output='js/compiled/tv.js'  %}
    <script src="{{ asset_url }}"></script>
{% endjavascripts %}

{% if device['script'] is defined %}
  {% include device['script'] %}
{% endif %}

{#
<script>
	var _gaq=[['_setAccount','UA-XXXXX-X'],['_trackPageview']];
	(function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
	g.src=('https:'==location.protocol?'//ssl':'//www')+'.google-analytics.com/ga.js';
	s.parentNode.insertBefore(g,s)}(document,'script'));
</script>
#}

</body>
</html>